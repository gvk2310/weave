{
    "name": "react-redux-webpack",
    "version": "1.0.0",
    "description": "react-redux-webpack boilerplate",
    "main": "src/index.js",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack serve --open",
        "build": "cross-env NODE_ENV=production webpack",
        "start:prod": "cross-env NODE_ENV=production",
        "lint": "eslint ./src/**/**.js",
        "lint:fix": "eslint ./src/**/**.js --fix",
        "test": "jest",
        "test:watch": "npm run test -- --watch",
        "test:cover": "npm run test -- --coverage"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run test",
            "prepush": "npm run lint && npm run test"
        }
    },
    "keywords": [
        "react",
        "redux",
        "webpack",
        "jest",
        "boilerplate"
    ],
    "license": "",
    "devDependencies": {
        "@babel/cli": "^7.4.3",
        "@babel/core": "^7.4.3",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.3.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-syntax-import-meta": "^7.2.0",
        "@babel/plugin-transform-async-to-generator": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.2.0",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "@babel/register": "^7.0.0",
        "@babel/runtime": "^7.3.1",
        "@testing-library/react": "^10.0.4",
        "@webpack-cli/serve": "^1.3.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.0.0",
        "babel-loader": "^8.0.4",
        "clean-webpack-plugin": "^3.0.0",
        "core-js": "3",
        "css-loader": "^3.3.2",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.5.0",
        "eslint": "^5.12.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^4.0.0",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.15.0",
        "eslint-plugin-jsx-a11y": "^6.2.0",
        "eslint-plugin-prettier": "^3.0.0",
        "eslint-plugin-react": "^7.12.4",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^5.0.2",
        "html-webpack-plugin": "^5.2.0",
        "husky": "^3.1.0",
        "img-loader": "^3.0.1",
        "jest": "^24.9.0",
        "jest-dom": "^4.0.0",
        "node-sass": "^4.14.1",
        "postcss-loader": "^3.0.0",
        "prettier": "^1.19.1",
        "redux-mock-store": "^1.5.4",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.0.1",
        "svg-url-loader": "^5.0.0",
        "url-loader": "^3.0.0",
        "webpack": "^5.14.0",
        "webpack-cli": "^4.4.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {
        "@apollo/react-testing": "^3.1.4",
        "apollo-boost": "^0.4.9",
        "axios": "^0.19.2",
        "buffer": "^5.6.0",
        "bundle-loader": "^0.5.6",
        "cors": "^2.8.5",
        "cross-env": "^6.0.3",
        "crypto-js": "^4.0.0",
        "express": "^4.17.1",
        "graphql": "^15.0.0",
        "node-fetch": "^2.6.0",
        "react": "^16.12.0",
        "react-apollo": "^3.1.5",
        "react-dom": "^16.12.0",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2",
        "recharts": "^1.8.5",
        "redux": "^4.0.4",
        "redux-thunk": "^2.3.0",
        "universal-cookie": "^4.0.2"
    },
    "jest": {
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>src/setupTests.js"
        ],
        "moduleNameMapper": {
            "\\.(css|less)$": "<rootDir>/config/mocks/fileMock.js"
        },
        "setupFiles": [
            "<rootDir>/config/tests/jest.config"
        ],
        "transform": {
            "^.+\\.js$": "babel-jest",
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/mocks/fileTransformer.js"
        }
    }
}
