###PROXY:CLOVEUIPARENT###
location ~ (/mywizardplatform/:*) {
        rewrite /mywizardplatform/(.*)$ /$1 break;
        proxy_pass http://cloveplatformuiparent.ethan.svc.cluster.local:3000;
        proxy_read_timeout      90;
        proxy_connect_timeout   90;
        proxy_send_timeout      90;
        auth_ldap off;
}
###PROXY:CLOVEUIPARENT###